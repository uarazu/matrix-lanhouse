<h1>Painel Principal</h1>

<table>
  <thead>
    <tr>
      <th>Description 1</th>
      <th>Description 2</th>
      <th>Description 3</th>
      <th>Description 4</th>
      <th>Description 5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sliders.each do |slider| %>
      <tr>
        <td><%= slider.description_1 %></td>
        <td><%= slider.description_2 %></td>
        <td><%= slider.description_3 %></td>
        <td><%= slider.description_4 %></td>
        <td><%= slider.description_5 %></td>
        <td><%= link_to 'Show', slider %></td>
        <td><%= link_to 'Edit', edit_slider_path(slider) %></td>
        <td><%= link_to 'Destroy', slider, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Slider', new_slider_path %>
