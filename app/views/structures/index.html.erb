<h1>Listing structures</h1>

<table>
  <thead>
    <tr>
      <th>Left title</th>
      <th>Center title</th>
      <th>Center description</th>
      <th>Right title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @structures.each do |structure| %>
      <tr>
        <td><%= structure.left_title %></td>
        <td><%= structure.center_title %></td>
        <td><%= structure.center_description %></td>
        <td><%= structure.right_title %></td>
        <td><%= link_to 'Show', structure %></td>
        <td><%= link_to 'Edit', edit_structure_path(structure) %></td>
        <td><%= link_to 'Destroy', structure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Structure', new_structure_path %>
